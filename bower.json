{
  "name": "angular-brunch-seed",
  "repo": "scotch/angular-brunch-seed",
  "version": "0.3.0",
  "main": "_public/js/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "console-polyfill": "~0.1.0",
    "angular": "~1.0.8",
    "angular-bootstrap": "*",
    "angular-cookies": "~1.0.8",
    "angular-loader": "~1.0.8",
    "angular-resource": "~1.0.8",
    "angular-sanitize": "~1.0.8",
    "angular-scenario": "~1.0.8",
    "angular-i18n": "~1.0.8",
    "moment": "*",
    "lodash": ">=2.4.1",
    "angular-ui-bootstrap": "*",
    "bootstrap-css": ">=3.0.0",
    "angular-growl": "*"
  },
  "overrides": {
    "angular-i18n": {
      "main": "angular-locale_it-it.js"
    },
    "angular-bootstrap": {
      "main": [
        "./ui-bootstrap-tpls.js"
      ]
    },
    "angular-growl": {
      "main": [
        "./build/angular-growl.js",
        "./build/angular-growl.min.css"
      ]
    },
    "angular-ui-bootstrap": {
      "main": [
        "./src/datepicker/datepicker.js"
      ]
    },
    "angular-ui-utils": {
      "main": [
        "./modules/validate/validate.js"
      ]
    },
    "bootstrap-datepicker": {
      "main": [
        "./js/bootstrap-datepicker.js",
        "./js/locales/bootstrap-datepicker.it.js",
        "./css/datepicker.css"
      ]
    },
    "angular-mocks": {
      "main": "README.md"
    },
    "angular-scenario": {
      "main": "README.md"
    },
    "angular-cookies": {
      "main": "README.md"
    },
    "moment": {
      "main": [
        "moment.js",
        "min/langs.min.js"
      ]
    },
    "restangular": {
      "main": "./dist/restangular.js"
    },
    "bootstrap-css": {
      "main": [
        "./css/bootstrap.css"
      , "./css/bootstrap-theme.css"
      ]
    }
  },
  "resolutions": {
    "angular": "~1.0.8"
  }
}
