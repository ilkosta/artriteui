{
  "name": "angular-brunch-seed",
  "repo": "scotch/angular-brunch-seed",
  "version": "0.3.0",
  "main": "_public/js/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "console-polyfill": "~0.1.0",
    "jquery": "*",
    
    "angular": "*",
    "angular-bootstrap": "*",
    "angular-cookies": "*",
    "angular-loader": "*",
    "angular-resource": "*",
    "angular-sanitize": "*",
    "angular-scenario": "*",
    
    "angular-i18n":  "*",
    "angular-ui-utils": "*",

    "moment":"*",
    "lodash": "*",
    "restangular": "1.1.3",
    "angular-ui-bootstrap": "*",

    "angular-strap": "*",

    "bootstrap": ">=3.0.0",
    "bootstrap-datepicker": "*",
    "bootstrap-select": "*",

    "angular-growl": "*"
  },
  "overrides": {
    
    "angular-i18n": { "main":"angular-locale_it-it.js" },
    "angular-bootstrap": {"main": [
      "./ui-bootstrap-tpls.js"
    ]},
    "angular-growl": {"main": [
      "./build/angular-growl.js",
      "./build/angular-growl.min.css"
    ]},
    "angular-ui-bootstrap": {"main": [
      "./src/datepicker/datepicker.js"
    ]},
    "angular-ui-utils": {"main": [
      "./modules/validate/validate.js"
    ]},
    "bootstrap-datepicker": {"main": [
      "./js/bootstrap-datepicker.js",
      "./js/locales/bootstrap-datepicker.it.js",
      "./css/datepicker.css"
    ]},
    "angular-mocks": {
      "main": "README.md"
    },
    "angular-scenario": {
      "main": "README.md"
    },
    "angular-cookies": {
      "main": "README.md"
    },
    "moment" : {"main": ["moment.js", "min/langs.min.js"]},
    "restangular" : {"main": "./dist/restangular.js"}
  }
}
